<h3><%= @title_search %></h3>
<ul class="thumbnails">
  <% @books.each do |book| %>
  <li class="book">
    <div class="thumbnail">
      <%= link_to image_tag( book.image, class: "img_book"), book_path(book.id) %>
      <div class="caption">
        <h5>
          <button class="fa fa-heart-o"></button>
          <%= book.title %>
        </h5>
      </div>
    </div>
  </li>
  <% end %>
  <%= will_paginate @books %>
</ul>
